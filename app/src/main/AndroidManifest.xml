<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bs.john_li.bsfslotmachine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BSSMTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BSSMActivity.MainActivity"/>
        <activity android:name=".BSSMActivity.Mine.SettingActivity"/>
        <activity android:name=".BSSMActivity.Mine.PersonalSettingActivity"/>
        <activity android:name=".BSSMActivity.Mine.WalletActivity"/>
        <activity android:name=".BSSMActivity.Mine.DiscountActivity"/>
        <activity android:name=".BSSMActivity.Mine.HistoryOrderActivity"/>
        <activity android:name=".BSSMActivity.Mine.CarListActivity"/>
    </application>

</manifest>